{
  "name": "torth",
  "displayName": "Torth",
  "description": "Syntax highlighting for Torth",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "torth",
        "aliases": [
          "Torth",
          "torth"
        ],
        "extensions": [
          ".torth"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "torth",
        "scopeName": "source.torth",
        "path": "./syntaxes/torth.tmLanguage.json"
      }
    ]
  }
}
