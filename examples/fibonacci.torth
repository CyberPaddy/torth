include "std"
function Fib int -> int :
  take limit in
  0   // Previous
  1   // Current
  1   // The counter for WHILE loop
  WHILE dup limit > DO
    take counter current previous in
    current             // Previous = Current
    previous current +  // Current = Previous + Current
    counter 1 +         // counter++
  DONE drop drop drop
  current
end

function Main :
  20  // Limit
  "The " puts dup putu ". fibonacci number is: " puts
  Fib putu "\n" puts  // Answer: 4181
end
